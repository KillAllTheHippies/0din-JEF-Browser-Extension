{
  "manifest_version": 3,
  "name": "JEF (Jailbreak Evaluation Testing)",
  "version": "1.0.0",
  "description": "Evaluate AI model outputs directly from ChatGPT, Gemini, Qwen, and DeepSeek using the Jailbreak Evaluation Framework",
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "https://chat.openai.com/*",
    "https://chatgpt.com/*",
    "https://gemini.google.com/*",
    "https://bard.google.com/*",
    "https://chat.qwen.ai/*",
    "https://chat.deepseek.com/*",
    "https://claude.ai/*",
    "https://codeium.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://chat.openai.com/*",
        "https://chatgpt.com/*",
        "https://gemini.google.com/*",
        "https://bard.google.com/*",
        "https://chat.qwen.ai/*",
        "https://chat.deepseek.com/*",
        "https://claude.ai/*"
      ],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "JEF",
    "default_icon": "icons/my-icon.png"
  },
  "icons": {
    "16": "icons/my-icon.png",
    "48": "icons/my-icon.png",
    "128": "icons/my-icon.png"
  }
}
